{
    "product_name": "qemu-x86_64-linux-min",
    "version": "3.0",
    "type": "standard",
    "target_cpu": "x86_64",
    "device_company": "qemu",
    "board": "qemu-x86_64-linux",
    "target_os": "ohos",
    "enable_ramdisk": true,
    "support_jsapi": false,
    "device_build_path": "device/qemu/x86_64_virt/linux",
    "subsystems": [
      {
        "subsystem": "build",
        "components": [
          { "component": "build_framework", "features":[] }
        ]
      },
      {
        "subsystem": "startup",
        "components": [
          { "component": "startup_l2", "features":[] },
          { "component": "init", "features":[] }
        ]
      },
      {
        "subsystem": "hiviewdfx",
        "components": [
          { "component": "hilog", "features":[] },
          { "component": "hitrace", "features":[] },
          { "component": "faultloggerd", "features":[] },
          { "component": "hisysevent", "features":[] },
          { "component": "hichecker", "features":[ "hichecker_support_asan=false" ] },
          { "component": "hiappevent", "features":[] }
        ]
      },
      {
        "subsystem": "distributedhardware",
        "components": [
          { "component": "device_manager", "features": [] }
        ]
      },
      {
        "subsystem": "security",
        "components": [
          { "component": "device_auth", "features":[] },
          { "component": "access_token", "features":[] },
          { "component": "huks", "features": ["huks_qemu_enabled = true"] }
        ]
      },
      {
        "subsystem": "commonlibrary",
        "components": [
          { "component": "c_utils", "features": [] },
          { "component": "ylong_runtime", "features": [] }
        ]
      },
      {
        "subsystem": "communication",
        "components": [
          { "component": "ipc", "features":[] },
          { "component": "dsoftbus", "features":["dsoftbus_get_devicename=false"] }
        ]
      },
      {
        "subsystem": "notification",
        "components": [
          { "component": "eventhandler", "features":[] }
        ]
      },
      {
        "subsystem": "systemabilitymgr",
        "components": [
          { "component": "samgr", "features":[] },
          { "component": "safwk", "features":[] }
        ]
      },
      {
        "subsystem": "developtools",
        "components": [
          { "component": "hdc", "features":[] }
        ]
      },
      {
        "subsystem": "securec",
        "components": [
          { "component": "thirdparty_bounds_checking_function", "features":[] }
        ]
      },
      {
        "subsystem": "device_qemu-x86_64-linux",
        "components": [
          { "component": "qemu_x86_64_linux_chipset", "features":[] }
        ]
      },
      {
        "subsystem": "resourceschedule",
        "components": [
          { "component": "ffrt", "features": [] },
          { "component": "frame_aware_sched", "features": [] }
        ]
      },
      {
        "subsystem": "ability",
        "components": [
          { "component": "idl_tool", "features":[] }
        ]
      },
      {
        "subsystem": "arkui",
        "components": [
          { "component": "ace_engine", "features": ["ace_engine_feature_enable_web = true", "ace_engine_feature_enable_accessibility = true"] }
        ]
      },
      {
        "subsystem": "graphic",
        "components": [
          { "component": "graphic_2d", "features": ["graphic_2d_feature_rs_enable_eglimage = true", "graphic_2d_feature_use_texgine = true"] },
          { "component": "graphic_surface", "features": [] }
        ]
      }
    ]
  }
